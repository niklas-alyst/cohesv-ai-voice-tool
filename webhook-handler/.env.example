# Webhook Handler Secrets
# Copy this file to .env and fill in the actual values
# These secrets will be stored in AWS Secrets Manager for production use

# Twilio Account Sid
TWILIO_ACCOUNT_SID=your-account-sid-here
# Twilio Auth Token (used for webhook signature validation)
TWILIO_AUTH_TOKEN=your-twilio-auth-token-here

# Wunse API Configuration (used for customer lookup)
WUNSE_API_BASE_URL=https://api.wunse.com
WUNSE_API_KEY=your-wunse-api-key-here
