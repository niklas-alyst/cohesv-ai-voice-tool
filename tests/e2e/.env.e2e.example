# E2E Test Configuration for Dev Environment
# Copy this file to .env.e2e and fill in actual values

# AWS Configuration
AWS_REGION=ap-southeast-2
AWS_PROFILE=cohesv

# S3 Configuration
E2E_S3_BUCKET=cohesv-ai-voice-tool-dev

# API Endpoints (Get these from CloudFormation stack outputs)
# For webhook URL: aws cloudformation describe-stacks --stack-name dev-ai-voice-shared --query 'Stacks[0].Outputs[?OutputKey==`WebhookApiUrl`].OutputValue' --output text
# For data API URL: aws cloudformation describe-stacks --stack-name dev-ai-voice-shared --query 'Stacks[0].Outputs[?OutputKey==`DataApiUrl`].OutputValue' --output text
E2E_WEBHOOK_URL=https://YOUR_API_ID.execute-api.ap-southeast-2.amazonaws.com/webhook
E2E_DATA_API_URL=https://YOUR_API_ID.execute-api.ap-southeast-2.amazonaws.com
E2E_DATA_API_KEY=your-data-api-key-here

# Twilio Configuration
# Get from AWS Secrets Manager: aws secretsmanager get-secret-value --secret-id dev/ai-voice-tool/twilio --query SecretString --output text
TWILIO_AUTH_TOKEN=your-twilio-auth-token-here
TWILIO_ACCOUNT_SID=AC1234567890abcdef1234567890abcdef
TWILIO_WHATSAPP_NUMBER=whatsapp:+61468035449

# Test Customer Configuration
# This customer must exist in S3 bucket at customers.json
E2E_TEST_COMPANY_ID=test-company-e2e
E2E_TEST_CUSTOMER_ID=test-customer-e2e
E2E_TEST_PHONE_NUMBER=whatsapp:+15555555555

# Test Configuration
E2E_TEST_TIMEOUT_SECONDS=90
E2E_CLEANUP_ON_FAILURE=true
